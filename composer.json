{
    "name": "kodinus/dynamicgen-qris",
    "description": "QRIS Generator & Parser (Dynamic & Static) untuk Laravel dan PHP, oleh PT Kodingin Digital Nusantara",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kodinus\\DynamicGenQris\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodinus\\DynamicGenQris\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "authors": [
        {
            "name": "PT Kodingin Digital Nusantara",
            "email": "halo@kodinus.id"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kodinus\\DynamicGenQris\\QrisServiceProvider"
            ],
            "aliases": {
                "Qris": "Kodinus\\DynamicGenQris\\Facades\\Qris"
            }
        }
    }
}